@model IEnumerable<WorldOfHalalWebUI.Models.Hotel>

@{
    ViewBag.Title = "HotelList";
}

<h2>Hotel List</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.HotelName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.HotelLocation)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.HotelName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.HotelLocation)
        </td>
    </tr>
}

</table>
